<main>
  {{#with page_header}}
    <title>{{title}}</title>
    <h1 class="text-uppercase text-center">{{title}}</h1>
    <p class="text-muted text-uppercase text-center">{{description}}</p>
  {{/with}}
  <br>
  {{#with section}}
    {{#with about_us}}
      <div class="card">
        <div class="card-body">
          {{#with title_list}}
          <div class="display-6 text-end">{{main}}</div>
          <br>
          <h2 class="text-end">{{sub}}</h2>
          {{/with}}
          <br>
          <button class="btn btn-outline-info">{{button.text}}</button>
        </div>
      </div>
    {{/with}}
    <br>
    <div class="card">
      <div class="card-body">
        {{#with advantage}}
          <div class="text-muted display-5">{{title}}</div>
          <br>
          <div class="card">
            <div class="card-body">
              {{#with efficiency}}
                <div class="text-muted display-4">{{title}}</div>
                <br>
                <h6 class="text-muted">{{sub_title}}</h6>
                <br>
                <p class="text-muted">{{description}}</p>
                <br>
                <button class="btn btn-outline-warning">{{button}}</button>
              {{/with}}
            </div>
          </div> 
          <br>
          <div class="card">
            <div class="card-body">
              {{#with innovation}}
                <div class="text-center text-muted display-4">{{title}}</div>
                <br>
                <h6 class="text-center text-muted">{{sub_title}}</h6>
                <br>
                <p class="text-center text-muted">{{description}}</p>
                <br>
                <button class="btn btn-outline-warning">{{button}}</button>
              {{/with}}
            </div>
          </div> 
          <br> 
          <div class="card">
            <div class="card-body">
              {{#with collaboration}}
                <div class="text-end text-muted display-4">{{title}}</div>
                <br>
                <h6 class="text-end text-muted">{{sub_title}}</h6>
                <br>
                <p class="text-end text-muted">{{description}}</p>
                <br>
                <button class="btn btn-outline-warning">{{button}}</button>
                <br> 
              {{/with}}
            </div>
          </div> 
        {{/with}}
      </div>
    </div>
    <br>  
    <div class="card">
      <div class="card-body">
        {{#with agreement}}
          <div class="display-5">{{title}}</div>
          <br>
          <div class="card">
            <div class="card-body">
              {{#with info}}
                <h5 class="text-muted">{{sub_title}}</h5>
                <br>
                <div class="card">
                  <div class="card-body">
                    {{#with detail}}
                      <p class="text-muted">{{text}}</p>
                      <br>
                      <button class="btn btn-success">{{button}}</button>
                    {{/with}}
                  </div>
                </div>
              {{/with}}
            </div>
          </div>
        {{/with}}
      </div>
    </div> 
  {{/with}}
</main>

