<main>
  {{#with heading}}
    <div class="card">
      <div class="card-body">
        {{#with main}}
          <title>{{title}}</title>
          <h1 class="text-uppercase">{{title}}</h1>
          <p class="text-muted">{{text}}</p>
          {{#with button_text}}
            <p><button class="btn btn-primary">{{solid}}</button></p>
            <br>
            <p><button class="btn btn-outline-primary">{{outline}}</button></p>
          {{/with}}
        {{/with}}
      </div>
    </div>
    <br>
    <div class="card">
      <div class="card-body">
        {{#with second}}
          <title>{{title}}</title>
          <h2 class="text-uppercase">{{title}}</h2>
          <p class="text-muted">{{text}}</p>
          {{#with button_text}}
            <p><button class="btn btn-secondary">{{solid}}</button></p>
            <p><button class="btn btn-outline-secondary">{{outline}}</button></p>
          {{/with}}
        {{/with}}
      </div>
    </div>  
    <br>
    <div class="card">
      <div class="card-body">
        {{#with subblock}}
          <title>{{title}}</title>
          <h3 class="text-uppercase">{{title}}</h3>
          <p class="text-muted">{{text}}</p>
          {{#with button_text}}
            <p><button class="btn btn-dark">{{solid}}</button></p>
            <p><button class="btn btn-outline-dark">{{outline}}</button></p>
          {{/with}}
        {{/with}}
      <br> 
      </div>
    </div>
  {{/with}}
  <br>
  <br>
  <div class="card">
    <div class="card-body">
      {{#with nested_display}}
        <div class="display-1">{{title}}</div>
        <br>
        <div class="card">
          <div class="card-body">
            {{#with children_display}}
              <div class="display-4">{{title}}</div>
              <p class="text-muted text-capitalize">{{text}}</p>
              <button class="btn btn-outline-warning">{{button.text}}</button>
            {{/with}}
          </div>
        </div>
        <br>
        <div class="display-6">{{header_text}}</div>
        <br>
        <button class="btn btn-info">{{button.text}}</button>
      {{/with}}
    </div>
  </div>
  <br>
  <div class="card">
    <div class="card-body">
      {{#with paragraph}}
        <p class="text-uppercase">{{title}}</p>
        <p class="text-lowercase">{{text}}</p>
        {{#with button}}
          <a href="{{url}}" class="btn btn-link">{{text}}</a>
          <br>
        {{/with}}
        <br>
        <div class="card">
          <div class="card-body">
            {{#each list_name}}
              <ul>
                <li><a href="{{url}}" class="btn btn-link">{{text}}</a></li>
              </ul>
            {{/each}}
          </div>
        </div>
      {{/with}}
    </div>
  </div>
</main>